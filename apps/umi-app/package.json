{
  "private": true,
  "name": "umi-app",
  "scripts": {
    "start": "umi dev",
    "dev": "PORT=3000 umi dev",
    "build": "umi build",
    "postinstall": "umi setup",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@infras/ui": "workspace:*",
    "@infras/shared": "workspace:*",
    "react": "17.x",
    "react-dom": "17.x"
  },
  "dependenciesMeta": {
    "@infras/ui": {
      "injected": true
    }
  },
  "devDependencies": {
    "@infras/eslint-config-local": "workspace:*",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "umi": "^4.0.0",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
