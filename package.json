{
  "private": true,
  "scripts": {
    "build:pkg": "pnpm run prepare -r"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "^1.1.2"
  },
  "pnpm": {
    "overrides": {
      "@remix-run/dev@1>esbuild": "0.14.21"
    }
  },
  "turbo": {
    "npmClient": "pnpm",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".umi/**",
          ".next/**"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ]
      },
      "dev": {
        "cache": false
      }
    }
  }
}
